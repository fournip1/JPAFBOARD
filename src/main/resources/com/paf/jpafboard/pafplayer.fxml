<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.paf.jpafboard.PafplayerController">
   <children>
      <AnchorPane fx:id="pane" layoutX="58.0" layoutY="86.0" prefHeight="228.0" prefWidth="452.0">
         <children>
            <Label fx:id="songLabel" alignment="CENTER" layoutY="14.0" prefHeight="41.0" prefWidth="471.0" text="PAF player" AnchorPane.topAnchor="14.0">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <ProgressBar fx:id="songProgressBar" layoutY="69.0" prefHeight="20.0" prefWidth="471.0" progress="0.0" />
            <Button fx:id="playButton" layoutY="157.0" mnemonicParsing="false" onAction="#playMedia" prefHeight="24.0" prefWidth="95.0" text="Play">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
            <Button fx:id="pauseButton" layoutX="95.0" layoutY="157.0" mnemonicParsing="false" onAction="#pauseMedia" prefHeight="30.0" prefWidth="95.0" text="Pause">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
            <ComboBox fx:id="speedBox" layoutX="268.0" layoutY="102.0" onAction="#changeSpeed" prefHeight="24.0" prefWidth="120.0" promptText="Speed" />
            <Slider fx:id="volumeSlider" layoutX="73.0" layoutY="106.0" prefHeight="16.0" prefWidth="159.0" value="100.0" />
            <Button fx:id="stopButton" layoutX="190.0" layoutY="157.0" mnemonicParsing="false" onAction="#stopMedia" prefHeight="30.0" prefWidth="95.0" text="Stop">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
            <Button fx:id="previousButton" layoutX="281.0" layoutY="157.0" mnemonicParsing="false" onAction="#previousMedia" prefHeight="30.0" prefWidth="95.0" text="Previous">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
            <Button fx:id="nextButton" layoutX="376.0" layoutY="157.0" mnemonicParsing="false" onAction="#nextMedia" prefHeight="30.0" prefWidth="95.0" text="Next">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
